  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebs/etherscan-cli/issues"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.7.5",
    "commander": "^2.9.0",
    "etherscan-api": "^3.0.5",
    "linger": "0.0.3",
    "marked": "^0.3.6",
    "marked-man": "^0.1.5",
    "prettyjson": "^1.1.3"
  },
  "devDependencies": {
    "chai": "~1.10.0",
    "changelog": "^1.0.7",
    "child-process-promise": "^2.1.3",
    "jshint": "^2.9.4",
    "mocha": "^3.1.0"
  }
